{
  "version": 2,
  "builds": [
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "templates/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "templates/index.html"
    },
    {
      "src": "/resources",
      "dest": "templates/resources.html"
    }
  ]
}