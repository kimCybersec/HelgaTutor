{
  "version": 2,
  "builds": [
    {
      "src": "frontend/public/**",
      "use": "@vercel/static"
    },
    {
      "src": "frontend/templates/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "frontend/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/templates/index.html"
    }
  ]
}